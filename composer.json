{
    "name": "rossriley/bolt-auth-active-directory",
    "description": "Active Directory Plugin for BoltAuth",
    "type": "bolt-extension",
    "keywords": [
        "Active Directory",
        "login",
        "auth",
        "oauth"
    ],
    "require": {
        "php": ">=5.5.9",
        "bolt/bolt": "^3.2",
        "ramsey/uuid": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ross Riley",
            "email": "riley.ross@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "name": "bolt",
            "type": "composer",
            "url": "https://extensions.bolt.cm/satis/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\BoltAuth\\ActiveDirectory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\BoltAuth\\ActiveDirectory\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\BoltAuth\\ActiveDirectory\\Extension",
        "bolt-icon": "active-directory.svg.png"
    }
}
